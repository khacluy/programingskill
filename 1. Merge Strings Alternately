/**
 * @param {string} word1
 * @param {string} word2
 * @return {string}
 */
var mergeAlternately = function(word1, word2) {
    let kq = "";
  let dodaimax = word1.length > word2.length?word1.length:word2.length;
  for (let i = 0; i < dodaimax; i++) {
    if (i < word1.length) {
      kq =kq + word1[i];
    }
    if (i < word2.length) {
      kq =kq + word2[i];
    }
  }
  return kq;
};
